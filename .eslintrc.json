{
  "root": true,
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "extends": ["plugin:@typescript-eslint/recommended"],
      "parser": "@typescript-eslint/parser",
      "plugins": ["@typescript-eslint"],
      "rules": {
        "@typescript-eslint/no-non-null-assertion": 0
      }
    },
    {
      "files": ["*.js"],
      "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
      }
    }
  ]
}
