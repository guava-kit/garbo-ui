---
import { CodeExample } from '../../components/code-example'
import { GButton } from '@lingjhf/guava'

const code = `
<GButton disabled>Button</GButton>
<GButton type="primary" disabled>Button</GButton>
<GButton type="success" disabled>Button</GButton>
<GButton type="warn" disabled>Button</GButton>
<GButton type="danger" disabled>Button</GButton>
`
---

<CodeExample client:only="solid-js" code={code}>
  <GButton client:only="solid-js" disabled>Button</GButton>
  <GButton client:only="solid-js" type="primary" disabled>Button</GButton>
  <GButton client:only="solid-js" type="success" disabled>Button</GButton>
  <GButton client:only="solid-js" type="warn" disabled>Button</GButton>
  <GButton client:only="solid-js" type="danger" disabled>Button</GButton>
</CodeExample>
