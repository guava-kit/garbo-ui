---
import { CodeExample } from '../../components/code-example'
import { GAlert } from '@lingjhf/guava'

const code = `
<GAlert />
<GAlert type="success" />
<GAlert type="warn" />
<GAlert type="danger" />
`
---

<CodeExample client:only="solid-js" code={code} language="jsx">
  <GAlert client:only="solid-js" />
  <GAlert client:only="solid-js" type="success" />
  <GAlert client:only="solid-js" type="warn" />
  <GAlert client:only="solid-js" type="danger" />
</CodeExample>
