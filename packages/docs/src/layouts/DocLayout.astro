---
import BaseLayout from './BaseLayout.astro'
import { GConfigProvider } from '@lingjhf/guava'
import { NavigationBar,NavigationBarItem } from './NavigationBar'

export interface Props {
  pageTitle: string
}
const { pageTitle } = Astro.props

const items:NavigationBarItem[] = [
  {title:""}
]
---

<BaseLayout pageTitle={pageTitle}>
  <GConfigProvider dark client:only="solid-js">
    <div class="w-full h-full flex">
      <div class="w-300px">
        <NavigationBar items={navs} />
      </div>
    </div>
  </GConfigProvider>
</BaseLayout>
